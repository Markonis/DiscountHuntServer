<p id="notice"><%= notice %></p>

<h1>Listing Friends Rankings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friends_rankings.each do |friends_ranking| %>
      <tr>
        <td><%= friends_ranking.user %></td>
        <td><%= link_to 'Show', friends_ranking %></td>
        <td><%= link_to 'Edit', edit_friends_ranking_path(friends_ranking) %></td>
        <td><%= link_to 'Destroy', friends_ranking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friends ranking', new_friends_ranking_path %>
